<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.gpsp"
		name="Nintendo - Game Boy Advance (gpSP)"
		version="0.91.0.34"
		provider-name="Exophase">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms></platforms>
		<extensions>gba|bin|agb|gbz</extensions>
		<supports_vfs>false</supports_vfs>
		<supports_standalone>false</supports_standalone>
		<requires_opengl>false</requires_opengl>
	</extension>
	<extension point="xbmc.addon.metadata">
		<license>GPLv2</license>
		<platform>@PLATFORM@</platform>
		<source>https://github.com/kodi-game/game.libretro.gpsp</source>
		<assets>
			<icon>resources/icon.png</icon>
			<screenshot>resources/screenshot-01.jpg</screenshot>
			<screenshot>resources/screenshot-02.jpg</screenshot>
			<screenshot>resources/screenshot-03.jpg</screenshot>
		</assets>
		<summary lang="da_DK">Nintendo - Game Boy Advance (gpSP)</summary>
		<summary lang="en_GB">Nintendo - Game Boy Advance (gpSP)</summary>
		<summary lang="es_ES">Nintendo - Game Boy Advance (gpSP)</summary>
		<description lang="da_DK">gpSP er en Game Boy Advance-emulator baseret på notaz&apos;s fork af gpSP med yderligere kodebaseforbedringer.</description>
		<description lang="en_GB">gpSP is a Game Boy Advance emulator based on notaz&apos;s fork of gpSP with additional codebase improvements.</description>
		<description lang="es_ES">gpSP es un emulador de Game Boy Advance basado en la bifurcación de notaz de gpSP con mejoras adicionales en el código base.</description>
		<description lang="zh_CN">gpSP 是一款基于 notaz 的 gpSP 分支的 Game Boy Advance 模拟器，具有额外的代码库改进。</description>
	</extension>
</addon>
